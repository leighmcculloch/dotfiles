{
  "network": {
    "allowedDomains": [
      "github.com",
      "*.github.com",
      "api.github.com"
    ],
    "deniedDomains": [],
    "allowUnixSockets": [],
    "allowLocalBinding": false
  },
  "filesystem": {
    "denyRead": [
      "~/.ssh",
      "~/.zenv_apikey_*",
      "~/Desktop/",
      "~/Documents/",
      "~/Filing*/",
      "~/Library/"
    ],
    "allowWrite": [
      ".",
      "/tmp",
      "~/Code/"
    ],
    "denyWrite": [
      ".env"
    ]
  },
  "ignoreViolations": {},
  "enableWeakerNestedSandbox": false
}
